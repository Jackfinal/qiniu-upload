{
    "name": "laravel-admin-ext/qiniu-upload",
    "description": "upload big files for qiniu",
    "type": "library",
    "keywords": ["laravel-admin", "extension"],
    "homepage": "https://github.com/laravel-admin-ext/qiniu-upload",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "jack",
            "email": "final_m@foxmail.com"
        }
    ],
    "require": {
